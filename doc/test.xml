      <is-reachability>true</is-reachability>
      <is-ctl>true</is-ctl>
      <is-ltl>true</is-ltl>
    </tags>
    <formula>
      <impossibility>
			<conjunction>
			  <is-fireable>
				 <transition>T0</transition>
				 <transition>T1</transition>
			  </is-fireable>
			  <is-fireable>
				 <transition>T2</transition>
				 <transition>T3</transition>
			  </is-fireable>
			</conjunction>
      </impossibility>
    </formula>
  </property>

