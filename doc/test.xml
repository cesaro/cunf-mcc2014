  </property>

  <property>
    <id>invariant (not deadlock) </id>
    <description>Automatically generated formula.</description>
    <tags>
      <is-structural>false</is-structural>
      <is-reachability>true</is-reachability>
      <is-ctl>true</is-ctl>
      <is-ltl>true</is-ltl>
    </tags>
    <formula>
      <invariant>
		  <negation>
			 <deadlock/>
		  </negation>
      </invariant>
    </formula>
  </property>

  <property>
    <id>impossibility (is-firable and is-firable)</id>
    <description>Automatically generated formula.</description>
