    <formula>
      <impossibility>
        <is-fireable>
          <transition>T0</transition>
          <transition>T1</transition>
          <transition>T2</transition>
        </is-fireable>
      </impossibility>
    </formula>
  </property>

  <property>
    <id>is-fireable</id>
